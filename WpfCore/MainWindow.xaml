<Window x:Class="WpfCore.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfCore"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox Text="{Binding Text1}"  HorizontalAlignment="Left" Height="47" Margin="62,50,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="104"/>
        <Button Content="Button" HorizontalAlignment="Left" Height="47" Margin="388,50,0,0" VerticalAlignment="Top" Width="95" Click="Button_Click"/>

        <ProgressBar Value="{Binding Num1}" Maximum="100" HorizontalAlignment="Left" Height="47" Margin="62,134,0,0" VerticalAlignment="Top" Width="293"/>
        <DataGrid HorizontalAlignment="Left" Height="125" Margin="62,217,0,0" VerticalAlignment="Top" Width="421"/>

    </Grid>
</Window>
